package aula7;

public class MainPessoa {

	public static void main(String[] args) {
		
		pessoa p1 = new pessoa();
			p1.setNome("Lima rodrigues");
			p1.setIdade(25);
			p1.setCPF("114.555.444-01");
			p1.setPeso(98.8);
			p1.setTamanho(1.80);
			
			p1.acordar();
			p1.calado();
			
			
			System.out.println(p1.getNome() +  " " +  p1.getIdade()  +  " " +  p1.getCPF()  +  " " + p1.getPeso()  +  " " + p1.getTamanho());

		

	}

}
